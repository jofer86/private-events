<h1>Events Attended by <%= @user.name %> </h1>
<h3>Previously attended events:</h3>
<% @past_events.each do |event| %>
  <ul>
    <li> <%= event.description %> | <%= event.e_date %> | <%= event.creator.name %> </li>
  </ul>
<% end %>
<h3>Events to attend:</h3>
<% @upcomming_events.each do |event| %>
  <ul>
    <li> <%= event.description %> | <%= event.e_date %> | <%= event.creator.name %> </li>
  </ul>
<% end %>